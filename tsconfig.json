{
	"compilerOptions": {
		"target": "ESNext",
		"skipLibCheck": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"noEmit": true,
		"noFallthroughCasesInSwitch": true,
		"esModuleInterop": true,
		"moduleResolution": "node",
		"module": "CommonJS"
	},
	"include": ["bin/*", "scripts/*", "server/*"]
}
